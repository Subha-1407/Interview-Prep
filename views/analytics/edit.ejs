<%- include('../partials/header'); %>



<div class="container">
    <h1 style="text-align: center;">Edit your experince</h1>

    <div style="width: 30%;margin: 0 auto ;">
        <form action="/analytics/<%= company_id %>/posts/<%= post._id %>?_method=PUT" method="POST">        
            <div class="form-group">
                <textarea class="form-control" name="post[text]" id="" cols="30" rows="15"><%= post.text %></textarea>
                
            </div>
            

            <div class="form-group">
                <button>Submit</button>
            </div>
        
        
        </form>
    
        <p style="margin: 0 auto;">
            <a class="btn btn-primary" href="/analytics/<%= company_id %>">Back</a>
        </p>
    </div>
</div>




<%- include('../partials/footer'); %>